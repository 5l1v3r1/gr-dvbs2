###################################################
 -->
<block>
  <name>LDPC Encoder</name>
  <key>dvbs2_ldpc_bb</key>
  <category>[DVB]/DVB-S2 VCM</category>
  <import>import dvbs2</import>
  <make>dvbs2.ldpc_bb()</make>
  <sink>
    <name>in</name>
    <type>byte</type>
  </sink>
  <source>
    <name>out</name>
    <type>byte</type>
  </source>
</block>
