<?xml version="1.0"?>
<!--
###################################################
## BCH Encoder
###################################################
 -->
<block>
  <name>BCH Encoder</name>
  <key>dvbs2_bch_bb</key>
  <category>[DVB]/DVB-S2</category>
  <import>import dvbs2</import>
  <make>dvbs2.bch_bb()</make>
  <sink>
    <name>in</name>
    <type>byte</type>
  </sink>
  <source>
    <name>out</name>
    <type>byte</type>
  </source>
</block>
